datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model Internship {
    id          String    @id @default(cuid())
    start       DateTime
    end         DateTime?
    title1      String
    title2      String?
    description String
    createdAt   DateTime  @default(now())
    updatedAt   DateTime  @updatedAt
}

model Education {
    id          String    @id @default(cuid())
    start       DateTime
    end         DateTime?
    title1      String
    title2      String?
    description String
    createdAt   DateTime  @default(now())
    updatedAt   DateTime  @updatedAt
}
